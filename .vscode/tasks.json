{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-std=c++17",
                "-Wall",
                "-fdiagnostics-color=always",
                "-O0",
                "-I\"${workspaceFolder}/final_project/src\"",
                "-g",
                "${workspaceFolder}/final_project/src/main.cpp",
                "-o",
                "${workspaceFolder}/final_project/main_debug"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}